<section>
    <h1>User List</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            {{#each users}}
            <tr>
                <td><a href="/users/{{_id}}" class="user-email">{{email}}</a></td>
                <td>{{firstName}}</td>
                <td>{{lastName}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</section>