<div class="row">
  <div class="col-sm-12 col-md-4">
    <form action="/login/signupuser" method="POST" id="signup-form" name="signup-form">
    
      <label for="firstName">First Name</label>
      <input type="text" id="firstName" name="firstName" required />
      <br>
      
      <label for="lastName">Last Name</label>
      <input type="text" id="lastName" name="lastName" required />
      <br>
      
      <label for="email">Email</label>
      <input type="email" id="email" name="email" required />
      <br>
      
      <label for="passwordPlainText">Password</label>
      <input type="password" id="passwordPlainText" name="passwordPlainText" required />
      <br>
      
      <label for="phoneNumber">Phone Number</label>
      <input type="text" id="phoneNumber" name="phoneNumber" required />
      <br>
      
      <label for="bio">Bio</label>
      <textarea id="bio" name="bio"></textarea>
      <br>
      
      <input type="submit" value="Sign Up" />
    </form>
    <br>
    <a id="signoutLink" href="/login/signinuser">Existing Account? Sign In</a>
    <br>
    <a id="signoutLink" href="/login/signoutuser">Sign out</a>
  </div>
</div>
